{
  "settings": {
    "globalScrollAmount": 50,
    "globalScrollBehavior": "auto",
    "globalZoomAmount": 0.1,
    "keysSeparator": "|",
    "maxCommandLength": 3,
    "hideToolbar": false,
    "enableCustomShortcutKeys": true
  },
  "keys": {
    "H": {
      "command": "toggle-help"
    },
    "j": {
      "command": "scroll-down",
      "settings": {
        "scrollAmount": 50,
        "scrollBehavior": "auto"
      }
    },
    "k": {
      "command": "scroll-up",
      "settings": {
        "scrollAmount": 50,
        "scrollBehavior": "auto"
      }
    },
    "l": {
      "command": "scroll-left",
      "settings": {
        "scrollAmount": 50,
        "scrollBehavior": "auto"
      }
    },
    "h": {
      "command": "scroll-right",
      "settings": {
        "scrollAmount": 50,
        "scrollBehavior": "auto"
      }
    },
    "g|g": {
      "command": "scroll-to-top",
      "settings": {
        "scrollBehavior": "smooth"
      }
    },
    "G": {
      "command": "scroll-to-bottom",
      "settings": {
        "scrollBehavior": "smooth"
      }
    },
    "/": {
      "command": "trigger-search"
    },
    "b": {
      "command": "toggle-sidebar"
    },
    "t": {
      "command": "toggle-toolbar"
    },
    "+": {
      "command": "zoom-in",
      "settings": {
        "zoomAmount": 0.1
      }
    },
    "-": {
      "command": "zoom-out",
      "settings": {
        "zoomAmount": 0.1
      }
    },
    "=": {
      "command": "zoom-reset"
    },
    "o": {
      "command": "open-file"
    },
    "P": {
      "command": "print-pdf"
    },
    "i": {
      "command": "toggle-insert-text"
    },
    "d": {
      "command": "toggle-draw"
    },
    "I": {
      "command": "toggle-insert-image"
    },
    "n": {
      "command": "next-page"
    },
    "p": {
      "command": "previous-page"
    },
    ">": {
      "command": "rotate-clockwise"
    },
    "<": {
      "command": "rotate-counterclockwise"
    },
    "Control|j": {
      "command": "last-page"
    },
    "Control|k": {
      "command": "first-page"
    },
    "?": {
      "command": "show-document-info"
    },
    "|": {
      "command": "no-action"
    }
  }
}