{
  "settings": {
    "globalScrollAmount": 50,
    "globalScrollBehavior": "auto",
    "globalZoomAmount": 0.1,
    "keysSeparator": "|",
    "maxCommandKeysLength": 5,
    "hideToolbar": false,
    "numberPrefix": "<NUMBER_PREFIX>",
    "enableCustomShortcutKeys": true
  },
  "keys": {
    "H": {
      "command": "toggle-help"
    },

    "j": {
      "command": "scroll-down",
      "settings": {
        "scrollAmount": 50,
        "scrollBehavior": "auto"
      }
    },
    "k": {
      "command": "scroll-up",
      "settings": {
        "scrollAmount": 50,
        "scrollBehavior": "auto"
      }
    },
    "l": {
      "command": "scroll-left",
      "settings": {
        "scrollAmount": 50,
        "scrollBehavior": "auto"
      }
    },
    "h": {
      "command": "scroll-right",
      "settings": {
        "scrollAmount": 50,
        "scrollBehavior": "auto"
      }
    },
    "g|g": {
      "command": "scroll-to-top",
      "settings": {
        "scrollBehavior": "smooth"
      }
    },
    "G": {
      "command": "scroll-to-bottom",
      "settings": {
        "scrollBehavior": "smooth"
      }
    },
    "<NUMBER_PREFIX>G": {
      "command": "scroll-to-page",
      "settings": {
        "scrollBehavior": "smooth"
      }
    },
    "u": {
      "command": "scroll-half-page-up",
      "settings": {
        "scrollBehavior": "auto"
      }
    },
    "d": {
      "command": "scroll-half-page-down",
      "settings": {
        "scrollBehavior": "auto"
      }
    },

    "t": {
      "command": "toggle-toolbar"
    },
    "b": {
      "command": "toggle-sidebar"
    },
    "/": {
      "command": "trigger-search"
    },
    "L": {
      "command": "focus-page-number"
    },
    "+": {
      "command": "zoom-in",
      "settings": {
        "zoomAmount": 0.1
      }
    },
    "-": {
      "command": "zoom-out",
      "settings": {
        "zoomAmount": 0.1
      }
    },
    "=": {
      "command": "zoom-reset"
    },
    "o": {
      "command": "open-file"
    },
    "P": {
      "command": "print-pdf"
    },
    "S": {
      "command": "save-pdf"
    },
    "n": {
      "command": "next-page"
    },
    "p": {
      "command": "previous-page"
    },
    "i": {
      "command": "toggle-insert-text"
    },
    "D": {
      "command": "toggle-insert-draw"
    },
    "U": {
      "command": "toggle-insert-image"
    },
    "T": {
      "command": "toggle-secondary-toolbar"
    },

    "F": {
      "command": "presentation-mode"
    },
    "K": {
      "command": "first-page"
    },
    "J": {
      "command": "last-page"
    },
    ">": {
      "command": "rotate-clockwise"
    },
    "<": {
      "command": "rotate-counterclockwise"
    },
    "s": {
      "command": "text-selection-tool"
    },
    "e": {
      "command": "hand-tool"
    },
    "y": {
      "command": "enable-page-scrolling"
    },
    "v": {
      "command": "enable-vertical-scrolling"
    },
    "m": {
      "command": "enable-horizontal-scrolling"
    },
    "w": {
      "command": "enable-wrapped-scrolling"
    },
    "N": {
      "command": "no-spread"
    },
    "O": {
      "command": "odd-spread"
    },
    "E": {
      "command": "even-spread"
    },
    "?": {
      "command": "show-document-info"
    },

    "c": {
      "command": "cycle-cursor-tool"
    },
    "C": {
      "command": "cycle-scrolling-mode"
    },
    "Y": {
      "command": "cycle-spread-mode"
    },
    "|": {
      "command": "no-action"
    }
  }
}